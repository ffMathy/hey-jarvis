  "plugins": [
    {
      "type": "node-workspace",
      "merge": true,
      "updateAllPackages": false
    },
    {
      "type": "linked-versions",
      "groupName": "mcp",
      "components": [
        "mcp",
        "home-assistant-addon"
      ]
    }
  ]
