  "bootstrap-sha": "f011e1c114e60382ee1d20b2a5528f6cb3ac5bee"
    },
    "mcp": {
      "release-type": "node",
      "package-name": "mcp",
      "changelog-path": "CHANGELOG.md",
      "initial-version": "0.1.0",
  "component": "mcp",
  "bootstrap-sha": "f011e1c114e60382ee1d20b2a5528f6cb3ac5bee"
    },
    "elevenlabs": {
      "release-type": "node",
      "package-name": "elevenlabs",
      "changelog-path": "CHANGELOG.md",
