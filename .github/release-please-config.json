{
  "packages": {
    ".": {
      "release-type": "node",
      "package-name": "hey-jarvis",
      "changelog-path": "CHANGELOG.md",
      "initial-version": "0.1.0"
    },
    "home-assistant-addon": {
      "release-type": "node",
      "package-name": "home-assistant-addon",
      "extra-files": [
        {
          "type": "json",
          "path": "config.json",
          "jsonpath": "$.version"
        }
      ],
      "changelog-path": "CHANGELOG.md",
      "initial-version": "0.1.0"
    },
    "jarvis-mcp": {
      "release-type": "node",
      "package-name": "jarvis-mcp",
      "changelog-path": "CHANGELOG.md",
      "initial-version": "0.1.0"
    },
    "elevenlabs": {
      "release-type": "node",
      "package-name": "elevenlabs",
      "changelog-path": "CHANGELOG.md",
      "initial-version": "0.1.0"
    },
    "home-assistant-voice-firmware": {
      "release-type": "python",
      "changelog-path": "CHANGELOG.md",
      "initial-version": "0.1.0"
    }
  },
  "group-pull-request-title-pattern": "chore: release ${version}",
  "separate-pull-requests": false
}