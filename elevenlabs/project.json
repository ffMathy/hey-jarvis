{
  "name": "elevenlabs",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "elevenlabs/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "platform": "node",
        "outputPath": "dist/elevenlabs",
        "format": ["esm"],
        "bundle": false,
        "main": "elevenlabs/src/main.ts",
        "tsConfig": "elevenlabs/tsconfig.app.json",
        "assets": ["elevenlabs/src/assets"],
        "generatePackageJson": true,
        "esbuildOptions": {
          "sourcemap": true,
          "outExtension": {
            ".js": ".js"
          }
        }
      },
      "configurations": {
        "development": {},
        "production": {
          "generateLockfile": true,
          "esbuildOptions": {
            "sourcemap": false,
            "outExtension": {
              ".js": ".js"
            }
          }
        }
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "./.scripts/run-with-env.sh elevenlabs/op.env node dist/elevenlabs/main.js deploy",
        "cwd": ".",
        "env": {
          "NODE_OPTIONS": "--experimental-vm-modules"
        }
      }
    },
    "refresh": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "./.scripts/run-with-env.sh elevenlabs/op.env node dist/elevenlabs/main.js fetch",
        "cwd": ".",
        "env": {
          "NODE_OPTIONS": "--experimental-vm-modules"
        }
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "dependsOn": ["build"],
      "options": {
        "command": "./.scripts/run-with-env.sh elevenlabs/op.env nx run elevenlabs:test:jest",
        "cwd": ".",
        "env": {
          "NODE_OPTIONS": "--experimental-vm-modules"
        }
      }
    },
    "test:jest": {
      "__comment": "Do not run this directly - run 'test' instead.",
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "elevenlabs/jest.config.ts",
        "bail": false,
        "runInBand": false,
        "passWithNoTests": false
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  }
}
