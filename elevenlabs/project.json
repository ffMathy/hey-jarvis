{
  "name": "elevenlabs",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "elevenlabs/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "env:local": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./.scripts/generate-env-local.sh elevenlabs/op.env",
        "cwd": "."
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "cache": true,
      "inputs": ["default", "^default"],
      "options": {
        "command": "tsgo --noEmit --project {projectRoot}/tsconfig.json"
      }
    },
    "lint": {},
    "initialize": {
      "executor": "nx:run-commands",
      "cache": true,
      "outputs": [],
      "options": {
        "commands": ["bunx playwright install", "bunx playwright install-deps"],
        "parallel": false,
        "cwd": "."
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./.scripts/run-with-env.sh elevenlabs/op.env bun elevenlabs/src/main.ts deploy",
        "cwd": "."
      }
    },
    "deploy:test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./.scripts/run-with-env.sh elevenlabs/op.env bun elevenlabs/src/main.ts deploy --test",
        "cwd": "."
      }
    },
    "refresh": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./.scripts/run-with-env.sh elevenlabs/op.env bun elevenlabs/src/main.ts fetch",
        "cwd": "."
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "dependsOn": ["lint", "deploy:test"],
      "options": {
        "command": "./.scripts/run-with-env.sh elevenlabs/op.env elevenlabs/.scripts/test.sh",
        "cwd": "."
      }
    },
    "tunnel": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./.scripts/run-with-env.sh elevenlabs/op.env bun elevenlabs/tests/utils/tunnel-manager.ts",
        "cwd": "."
      }
    }
  }
}
