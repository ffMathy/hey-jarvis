{
  "name": "elevenlabs",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "elevenlabs/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "env:local": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./.scripts/generate-env-local.sh elevenlabs/op.env elevenlabs/.env.local",
        "cwd": "."
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "cache": true,
      "inputs": ["default", "^default"],
      "options": {
        "command": "tsgo --noEmit --project {projectRoot}/tsconfig.json"
      }
    },
    "lint": {},
    "initialize": {
      "executor": "nx:run-commands",
      "cache": true,
      "outputs": [],
      "options": {
        "commands": ["bunx playwright install", "bunx playwright install-deps"],
        "parallel": false,
        "cwd": "."
      }
    },
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "platform": "node",
        "outputPath": "dist/elevenlabs",
        "format": ["esm"],
        "bundle": false,
        "main": "elevenlabs/src/main.ts",
        "tsConfig": "elevenlabs/tsconfig.app.json",
        "assets": ["elevenlabs/src/assets"],
        "generatePackageJson": false,
        "esbuildOptions": {
          "sourcemap": true,
          "outExtension": {
            ".js": ".js"
          }
        }
      },
      "configurations": {
        "development": {},
        "production": {
          "generateLockfile": false,
          "esbuildOptions": {
            "sourcemap": false,
            "outExtension": {
              ".js": ".js"
            }
          }
        }
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "./.scripts/run-with-env.sh elevenlabs/op.env node dist/elevenlabs/main.js deploy",
        "cwd": ".",
        "env": {
          "NODE_OPTIONS": "--experimental-vm-modules"
        }
      }
    },
    "deploy:test": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "./.scripts/run-with-env.sh elevenlabs/op.env node dist/elevenlabs/main.js deploy --test",
        "cwd": ".",
        "env": {
          "NODE_OPTIONS": "--experimental-vm-modules"
        }
      }
    },
    "refresh": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "./.scripts/run-with-env.sh elevenlabs/op.env node dist/elevenlabs/main.js fetch",
        "cwd": ".",
        "env": {
          "NODE_OPTIONS": "--experimental-vm-modules"
        }
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "dependsOn": ["lint", "deploy:test"],
      "options": {
        "command": "rm -rf dist/elevenlabs-spec && ./.scripts/run-with-env.sh elevenlabs/op.env bun test elevenlabs/tests",
        "cwd": "."
      }
    },
    "tunnel": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./.scripts/run-with-env.sh elevenlabs/op.env bun elevenlabs/tests/utils/tunnel-manager.ts",
        "cwd": "."
      }
    },
    "build:test": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "platform": "node",
        "outputPath": "dist/elevenlabs-test",
        "format": ["esm"],
        "bundle": true,
        "main": "elevenlabs/tests/specs/agent-prompt.spec.ts",
        "additionalEntryPoints": [
          "elevenlabs/tests/**/*.spec.ts",
          "elevenlabs/tests/**/*.test.ts",
          "elevenlabs/tests/**/*.ts"
        ],
        "tsConfig": "elevenlabs/tsconfig.app.json",
        "assets": [
          {
            "input": "elevenlabs/src/assets",
            "glob": "**/*",
            "output": "./assets"
          }
        ],
        "esbuildOptions": {
          "sourcemap": true,
          "outExtension": {
            ".js": ".js"
          },
          "alias": {
            "@mcp": "./mcp"
          }
        },
        "skipTypeCheck": true,
        "external": []
      },
      "dependsOn": []
    }
  }
}
