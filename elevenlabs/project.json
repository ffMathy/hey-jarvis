{
  "name": "elevenlabs",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "elevenlabs/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "../.scripts/run-with-env.sh op.env npx @elevenlabs/cli agents push --no-ui",
        "cwd": "elevenlabs"
      }
    },
    "refresh": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'y' | ../.scripts/run-with-env.sh op.env npx @elevenlabs/cli agents pull --no-ui --all",
        "cwd": "elevenlabs"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "command": "./.scripts/run-with-env.sh elevenlabs/op.env nx run elevenlabs:test:jest",
        "cwd": "."
      }
    },
    "test:jest": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "elevenlabs/jest.config.ts",
        "bail": true,
        "runInBand": true
      }
    }
  }
}
